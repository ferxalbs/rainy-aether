{
    "name": "rainy-agents-server",
    "version": "0.1.0",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch index.ts",
        "build:ts": "tsc",
        "build:pkg": "pkg dist/index.js --targets node20-macos-arm64,node20-macos-x64,node20-linux-x64,node20-win-x64 --output ../../../src-tauri/binaries/rainy-agents-server",
        "build": "pnpm build:ts && pnpm build:pkg"
    },
    "dependencies": {
        "hono": "^4.6.0",
        "@hono/node-server": "^1.13.0",
        "inngest": "^3.48.1",
        "@inngest/agent-kit": "^0.13.2"
    },
    "devDependencies": {
        "tsx": "^4.19.0",
        "typescript": "^5.9.3",
        "pkg": "^5.8.1",
        "@types/node": "^22.0.0"
    },
    "pkg": {
        "assets": [
            "../workflows/**/*.js",
            "../brain/**/*.js",
            "../inngest/**/*.js"
        ],
        "scripts": [
            "dist/**/*.js"
        ]
    }
}